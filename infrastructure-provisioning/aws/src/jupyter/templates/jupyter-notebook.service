[Unit]
Description=Jupyter notebook

[Service]
Type=simple
PIDFile=/var/run/juython-notebook.pid
ExecStart=/usr/local/bin/jupyter notebook --config /home/ubuntu/.local/share/jupyter/jupyter_notebook_config.py
ExecStop=/bin/bash -c "for i in $(ps aux | grep jupyter | grep -v grep | awk '{print $2}'); do kill -9 $i; done"
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu

[Install]
WantedBy=multi-user.target