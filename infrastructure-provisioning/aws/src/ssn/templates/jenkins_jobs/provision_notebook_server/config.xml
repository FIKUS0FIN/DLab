<?xml version='1.0' encoding='UTF-8'?>
<project>
    <actions/>
    <description>Create Notebook Server</description>
    <keepDependencies>false</keepDependencies>
    <properties>
        <hudson.model.ParametersDefinitionProperty>
            <parameterDefinitions>
                <hudson.model.StringParameterDefinition>
                    <name>Service_base_name</name>
                    <description>Base name of your services</description>
                    <defaultValue></defaultValue>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>NOTEBOOK_NAME</name>
                    <description>Additional extra parameter</description>
                    <defaultValue>nb</defaultValue>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>SUBNET</name>
                    <description></description>
                    <defaultValue>172.31.131.0/24</defaultValue>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>SSH_Key_Name</name>
                    <description>Key name at AWS to be used for accessing Jupyter</description>
                    <defaultValue>BDCC-DSS-POC</defaultValue>
                </hudson.model.StringParameterDefinition>
                <hudson.model.ChoiceParameterDefinition>
                    <name>Region</name>
                    <description></description>
                    <choices class="java.util.Arrays$ArrayList">
                        <a class="string-array">
                            <string>us-west-2</string>
                            <string>us-west-1</string>
                            <string>eu-west-1</string>
                        </a>
                    </choices>
                </hudson.model.ChoiceParameterDefinition>
            </parameterDefinitions>
        </hudson.model.ParametersDefinitionProperty>
    </properties>
    <scm class="hudson.scm.NullSCM"/>
    <canRoam>true</canRoam>
    <disabled>false</disabled>
    <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
    <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
    <triggers/>
    <concurrentBuild>false</concurrentBuild>
    <builders>
        <hudson.tasks.Shell>
            <command>rand=`openssl rand -hex 10`;
                sudo docker run -i -v /project_images/${SSH_Key_Name}.pem:/root/keys/${SSH_Key_Name}.pem -v /home/ubuntu:/response -e &quot;request_id=$rand&quot; -e &quot;creds_region=$Region&quot; -e &quot;conf_service_base_name=${Service_base_name}&quot; -e &quot;notebook_name=${NOTEBOOK_NAME}&quot; -e &quot;creds_key_name=$SSH_Key_Name&quot; -e &quot;network_cidr=$SUBNET&quot; docker.epmc-bdcc.projects.epam.com/dlab-aws-jupyter --action create</command>
        </hudson.tasks.Shell>
    </builders>
    <publishers/>
    <buildWrappers/>
</project>
